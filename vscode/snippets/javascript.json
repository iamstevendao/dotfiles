{
  "print to console": {
    "prefix": "slog",
    "body": [
      "console.log('---> ${1:variable}:', ${1:variable});",
      "${0}"
    ],
    "description": "log output to console"
  },
  "info to console": {
    "prefix": "sinfo",
    "body": [
      "console.info('---> ${1:content}:');",
      "${0}"
    ],
    "description": "log info to console"
  },
  "warn to console": {
    "prefix": "swarn",
    "body": [
      "console.warn('***** ${1:content}');",
      "${0}"
    ],
    "description": "log warning to console"
  },
  "error to console": {
    "prefix": "serr",
    "body": [
      "console.error('!!!! ${1:variable}:', ${1:variable});",
      "${0}"
    ],
    "description": "log error to console"
  },
  "ring a bell": {
    "prefix": "sring",
    "body": [
      "console.log('ring a bell');"
    ],
    "description": "ring a bell"
  },
  "import statement": {
    "prefix": "simp",
    "body": [
      "import $2 from '$1';",
      "$3"
    ],
    "description": "import from"
  },
  "export default": {
    "prefix": "sexp",
    "body": [
      "export default {",
      "\tname: '$1'",
      "\t$2",
      "};"
    ],
    "description": "export default"
  },
  "vue data": {
    "prefix": "sdata",
    "body": [
      "data() {",
      "\treturn {",
      "\t\t${1:name}: ${2:value},",
      "\t}",
      "},",
      "$3"
    ],
    "description": "add data property"
  },
  "vue computed": {
    "prefix": "scomputed",
    "body": [
      "computed: {",
      "\t${1:name}() {",
      "\t\treturn ${2:data};",
      "\t}",
      "},"
    ],
    "description": "add computed property"
  }
}
